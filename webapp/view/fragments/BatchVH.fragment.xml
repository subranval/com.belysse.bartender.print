<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="batchvalueHelpDialog"
		title="{i18n>batch}"
		items="{/I_Batch}"
		search="_handleValueHelpSearch"
		confirm="_handleBatchVHClose"
		cancel="_handleBatchVHClose"
		multiSelect="true">
		<StandardListItem
			icon=""
			iconDensityAware="false"
			iconInset="false"
			title="{Batch}"
			description="" />
	</SelectDialog>
</core:FragmentDefinition>