<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="materialvalueHelpDialog"
		title="{i18n>material}"
		items="{/ZI_ProductVH}"
		search="_handleValueHelpSearch"
		confirm="_handleMaterialVHClose"
		cancel="_handleMaterialVHClose"
		multiSelect="true">
		<StandardListItem
			icon=""
			iconDensityAware="false"
			iconInset="false"
			title="{Product}"
			description="" />
	</SelectDialog>
</core:FragmentDefinition>